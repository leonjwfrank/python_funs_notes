    "def translate(phrase):\n",
    "    translation = \"\"\n",
    "    for letter in phrase:\n",
    "        if letter in \"AEIOUaeiou\":\n",
    "            translation += \"g\"\n",
    "        else:\n",
    "            translation += letter\n",
    "    return translation"
   ]
